         <event activity="initialize" name="event__initialize">
            <script>
// Obtener la cantidad de filas en la tabla original (Table4)
var sourceTable = xfa.resolveNode("Form.bdyMain.frmTableBlock.Table4");
var targetTable = xfa.resolveNode("Form.bdyMain.Subform2.Table2");

var rowCount = sourceTable.Row1.length;

// Eliminar filas existentes en Table2 (excepto la primera, si querés)
while (targetTable.Row1.length &gt; 1) {
    targetTable._Row1.removeInstance(1); // empieza en 1 para dejar la primera
}

// Agregar las filas necesarias
for (var i = 1; i &lt; rowCount; i++) {
    targetTable._Row1.addInstance(false);
}

// Copiar valores
for (var j = 0; j &lt; rowCount; j++) {
    var sourceField = sourceTable.Row1[j].BillingDocumentItemText;
    var targetField = targetTable.Row1[j].BillingDocumentItemText2;

    if (sourceField != null &amp;&amp; sourceField.rawValue != null) {
        targetField.rawValue = sourceField.rawValue;
    }
}
</script>

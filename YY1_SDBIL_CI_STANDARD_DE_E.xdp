                  <field name="Exentas" access="open" usehref=".#som($template.#subform.designer__stylesheet.TextFieldStyle19)">
                     <ui>
                        <textEdit/>
                     </ui>
                     <calculate override="disabled">
                        <script>

//var idx = this.parent.index;
//var idx = this.parent.parent.index; // Si estás dentro de un subformulario dentro de la fila

//if ($record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode.ConditionRateValue == "0.000" and $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode.ConditionType == "TTX1") 
//then
 // $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[1].ConditionAmount
//else
  //  0
//endif


var idx = this.parent.index
var j = 0
var resultado = 0
var found = 0

// 🔍 Buscar dentro del ítem (caso filetmp3.XML)
while (exists($record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j]) and found == 0) do
  if (
    $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j].ConditionType == "TTX1" and
    $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j].ConditionRateValue == "0.000"
  ) then
    resultado = $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[1].ConditionAmount
    found = 1
  endif
  j = j + 1
endwhile

// 🔍 Si no lo encontró en el ítem, buscar en los nodos globales (caso 90000260.XML)
if (found == 0) then
  if (
    exists($record.BillingDocumentNode.PricingConditions.PricingConditionNode[idx]) and
    $record.BillingDocumentNode.PricingConditions.PricingConditionNode[idx].ConditionType == "TTX1" and
    $record.BillingDocumentNode.PricingConditions.PricingConditionNode[idx].ConditionRateValue == "0.000"
  ) then
    resultado = $record.BillingDocumentNode.PricingConditions.PricingConditionNode[idx].ConditionAmount + $record.BillingDocumentNode.PricingConditions.PricingConditionNode[idx].ConditionBaseValue
  else
    resultado = 0
  endif
endif

// 💬 Asegurar que SIEMPRE se vea "0" si el resultado es 0
if (resultado == 0) then
  $ = "0"
else
  $ = resultado
endif
</script>

                     <calculate>
                        <script contentType="application/x-formcalc">
var idx = this.parent.index
var j = 0
var total = ""

while (exists($record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j])) do
  if ($record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j].ConditionTypeName == "Valor total") then
    total = $record.BillingDocumentNode.Items.BillingDocumentItemNode[idx].ItemPricingConditions.ItemPricingConditionNode[j].ConditionAmount
  endif
  j = j + 1
endwhile

$ = total
</script>
                     </calculate>

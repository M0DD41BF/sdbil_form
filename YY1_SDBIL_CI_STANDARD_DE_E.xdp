            <event activity="ready" ref="$layout" name="event__layout_ready">
               <script>// Set the form title as the combination of title and document ID, separated by colon

// The title depends on the existance of the 'CancelledBillingDocumentID'

// if the ID beginns with $ it is a Preview therefore Preview instead of Number displayed
var form_title

if ( hasValue(Form.frmHiddenGlobalFields.txtDocumentTitle) )
then 
	form_title = Form.frmHiddenGlobalFields.txtDocumentTitle
else
	// If this ID is set the current document is a cancellation invoice
	if ( hasValue(Form.frmHiddenGlobalFields.txtCancelledBillingDocumentID) )
	then
		form_title = Form.frmHiddenStaticTexts.lblCancellationInvoice.rawValue
	// Otherwise the current document is an invoice
	else 
		form_title = Form.frmHiddenStaticTexts.lblInvoice.rawValue
	endif
endif

if ( Form.frmHiddenGlobalFields.txtBillingSDDocumentCategory.rawValue == "PBD" )
then
	form_title = Form.frmHiddenGlobalFields.txtBillingSDDocumentCategoryName.rawValue
endif


if ( not hasValue(Form.frmHiddenGlobalFields.txtIsPreview) )
then
	//Set title on first page (always available)
	xfa.resolveNode("#pageSet[0].mst1.FirstPage.frmHeader.txtTitle") = Concat(form_title, ": ", Form.frmHiddenGlobalFields.txtDocumentID.rawValue)

	//Set title on subsequent pages (if available)
	var pageCount = xfa.layout.pageCount();

	if (pageCount &gt; "1")
	then
		xfa.resolveNode("#pageSet[0].mst2.SubsequentPage.frmHeader.txtTitle") = Concat(form_title, ": ", Form.frmHiddenGlobalFields.txtDocumentID.rawValue)
	endif
else
// instead of number write text Preview
	//Set title on first page (always available)
	xfa.resolveNode("#pageSet[0].mst1.FirstPage.frmHeader.txtTitle") = Concat(Form.frmHiddenStaticTexts.lblPreview.rawValue, ": ", form_title)

	//Set title on subsequent pages (if available)
	var pageCount = xfa.layout.pageCount();

	if (pageCount &gt; "1")
	then
		xfa.resolveNode("#pageSet[0].mst2.SubsequentPage.frmHeader.txtTitle") = Concat(Form.frmHiddenStaticTexts.lblPreview.rawValue, ": ", form_title)
	endif
endif
</script>
            </event>

var a = ['', 'Un ', 'Dos ', 'Tres ', 'Cuatro ', 'Cinco ', 'Seis ', 'Siete ', 'Ocho ', 'Nueve ', 'Diez ', 'Once ', 'Doce ', 'Trece ',
'Catorce ', 'Quince ', 'Dieciséis ', 'Diecisiete ', 'Dieciocho ', 'Diecinueve '];

var b = ['', '', 'Veinte ', 'Treinta ', 'Cuarenta ', 'Cincuenta ', 'Sesenta ', 'Setenta ', 'Ochenta ', 'Noventa '];

 

var curr = xfa.resolveNode("$record.BillingDocumentNode.CompanyCodeCurrency").value;
 



function inWords(num1) {


var pos = num1.indexOf(".");

var num;

var dec;

if(pos == -1)

{

num = num1;

dec = "00";

}

else

{

num = num1.substring(0, pos);

dec = num1.substring(pos +1, num1.length);

}

if ((num = num.toString()).length &gt; 9) return 'overflow';

n = ('000000000' + num).substr(-9).match(/^(\d{2})(\d{2})(\d{2})(\d{1})(\d{2})$/);

var str = '';

var str1 = 'Rupees ';

if (!n) return;

str += (n[1] != 0) ? (a[Number(n[1])] || b[n[1][0]] + ' ' + a[n[1][1]]) + 'Millones ' : '';

str += (n[2] != 0) ? (a[Number(n[2])] || b[n[2][0]] + ' ' + a[n[2][1]]) + 'Mil ' : '';

str += (n[3] != 0) ? (a[Number(n[3])] || b[n[3][0]] + ' ' + a[n[3][1]]) + 'Mil ' : '';

str += (n[4] != 0) ? (a[Number(n[4])] || b[n[4][0]] + ' ' + a[n[4][1]]) + 'Cientos ' : '';

//str += (n[5] != 0) ? ((str != '') ? 'and ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) + 'only ' : '';

//str += (n[5] != 0) ? ((str != '') ? ' ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) + str1 : '' ;

if(curr == "PYG"){

str += (n[5] != 0) ? ((str != '') ? ' ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) + 'Guaranies' : 'Guaranies' ;
}
if(curr == "USD"){

str += (n[5] != 0) ? ((str != '') ? ' ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) + 'Dolares Americanos' : 'Dolares Americanos' ;
}

if ((dec = dec.toString()).length &gt; 2) return 'overflow';

d = ('00' + dec).substr(-2).match(/^(\d{2})$/);

if (!d) return;

str += '';

str += (Number(d[1]) !== 0) ? ((str !== '') ? " y " : '') + (this.a[Number(d[1])] || this.b[d[1][0]] + ' ' + this.a[d[1][1]]) + 'Centavos' : ' ';

return str;

}

//}

 

var num1 = xfa.resolveNode("$record.BillingDocumentNode.TotalNetAmount").value;
this.rawValue = inWords(num1.toString());
